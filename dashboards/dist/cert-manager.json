{
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "links": [],
  "panels": [
    {
      "type": "row",
      "title": "Ready status",
      "panels": [],
      "collapsed": false,
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 24,
        "h": 1
      }
    },
    {
      "links": [],
      "repeatDirection": "h",
      "tags": [],
      "targets": [
        {
          "editorMode": "code",
          "expr": "sum(certmanager_certificate_ready_status == 1) by (issuer_group, issuer_kind, issuer_name, name, namespace, condition)",
          "range": false,
          "instant": true,
          "format": "table",
          "refId": "discard"
        },
        {
          "editorMode": "code",
          "expr": "max(certmanager_certificate_expiration_timestamp_seconds[$__interval]) by (issuer_group, issuer_kind, issuer_name, name, namespace) * 1000",
          "range": false,
          "instant": true,
          "format": "table",
          "refId": "expiration_time"
        },
        {
          "editorMode": "code",
          "expr": "max(certmanager_certificate_renewal_timestamp_seconds[$__interval]) by (issuer_group, issuer_kind, issuer_name, name, namespace) * 1000",
          "range": false,
          "instant": true,
          "format": "table",
          "refId": "renewal_time"
        }
      ],
      "transformations": [
        {
          "id": "merge",
          "options": {}
        },
        {
          "id": "organize",
          "options": {
            "indexByName": {
              "issuer_group": 0,
              "issuer_kind": 1,
              "issuer_name": 2,
              "namespace": 3,
              "name": 4,
              "condition": 5
            },
            "excludeByName": {
              "Time": true,
              "Value #discard": true
            },
            "renameByName": {
              "Value #expiration_time": "Expiration time",
              "Value #renewal_time": "Renewal time"
            }
          }
        }
      ],
      "transparent": false,
      "datasource": {
        "uid": "${DS_PROMETHEUS}"
      },
      "type": "table",
      "title": "Ready status",
      "gridPos": {
        "x": 0,
        "y": 1,
        "w": 24,
        "h": 24
      },
      "fieldConfig": {
        "defaults": {},
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "condition"
            },
            "properties": [
              {
                "id": "mappings",
                "value": [
                  {
                    "options": {
                      "False": {
                        "color": "red",
                        "index": 1,
                        "text": "Not ready"
                      },
                      "True": {
                        "color": "green",
                        "index": 0,
                        "text": "Ready"
                      }
                    },
                    "type": "value"
                  }
                ]
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Expiration time"
            },
            "properties": [
              {
                "id": "unit",
                "value": "dateTimeFromNow"
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "Renewal time"
            },
            "properties": [
              {
                "id": "unit",
                "value": "dateTimeFromNow"
              }
            ]
          }
        ]
      },
      "options": {
        "cellHeight": "md",
        "frameIndex": -1,
        "showHeader": true
      }
    },
    {
